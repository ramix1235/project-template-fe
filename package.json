{
  "name": "project",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:development": "tsc -b && vite build --mode development",
    "build:staging": "tsc -b && vite build --mode staging",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "csslint:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "formatter:fix": "prettier . --write",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "codegen:openapi": "rtk-query-codegen-openapi openapi-config.ts && prettier \"src/services/api/*/*.api.ts\" --write"
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@casl/react": "^4.0.0",
    "@mantine/core": "^7.12.1",
    "@mantine/form": "^7.12.1",
    "@mantine/hooks": "^7.12.1",
    "@mantine/notifications": "^7.12.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@tabler/icons-react": "^3.12.0",
    "async-mutex": "^0.5.0",
    "clsx": "^2.1.1",
    "i18next": "^23.14.0",
    "mantine-datatable": "^7.11.3",
    "mantine-form-zod-resolver": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rtk-query/codegen-openapi": "^1.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "@vitejs/plugin-react": "^4.3.1",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "meow": "^9.0.0",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.8.2",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-qrcode": "^0.2.3",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "packageManager": "pnpm@9.7.1"
}
